[
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "payroll_indonesia_section",
    "fieldtype": "Section Break",
    "insert_after": "personal_details", 
    "label": "Payroll Indonesia",
    "module": "Payroll Indonesia",
    "name": "Employee-payroll_indonesia_section",
    "collapsible": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "golongan",
    "fieldtype": "Link",
    "insert_after": "payroll_indonesia_section",
    "label": "Golongan",
    "module": "Payroll Indonesia",
    "name": "Employee-golongan",
    "options": "Golongan",
    "in_list_view": 1,
    "in_standard_filter": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "col_break_payroll_id",
    "fieldtype": "Column Break",
    "insert_after": "golongan",
    "module": "Payroll Indonesia",
    "name": "Employee-col_break_payroll_id"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "jabatan",
    "fieldtype": "Link",
    "insert_after": "col_break_payroll_id",
    "label": "Jabatan",
    "module": "Payroll Indonesia",
    "name": "Employee-jabatan",
    "options": "Jabatan"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "status_pajak",
    "fieldtype": "Select",
    "insert_after": "jabatan",
    "label": "Status Pajak",
    "module": "Payroll Indonesia",
    "name": "Employee-status_pajak",
    "options": "TK0\nTK1\nTK2\nTK3\nK0\nK1\nK2\nK3\nHB0\nHB1\nHB2\nHB3"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "jumlah_tanggungan",
    "fieldtype": "Int",
    "insert_after": "status_pajak",
    "label": "Jumlah Tanggungan",
    "module": "Payroll Indonesia",
    "name": "Employee-jumlah_tanggungan",
    "read_only": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "npwp_section",
    "fieldtype": "Section Break",
    "insert_after": "personal_bio",
    "label": "Tax Identifiers",
    "module": "Payroll Indonesia",
    "name": "Employee-npwp_section"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "npwp",
    "fieldtype": "Data",
    "insert_after": "npwp_section",
    "label": "NPWP",
    "module": "Payroll Indonesia",
    "name": "Employee-npwp",
    "in_standard_filter": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "upload_npwp",
    "fieldtype": "Attach",
    "insert_after": "npwp",
    "label": "Upload NPWP",
    "module": "Payroll Indonesia",
    "name": "Employee-upload_npwp"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "ktp",
    "fieldtype": "Data",
    "insert_after": "upload_npwp",
    "label": "KTP",
    "module": "Payroll Indonesia",
    "name": "Employee-ktp",
    "in_standard_filter": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "upload_ktp",
    "fieldtype": "Attach",
    "insert_after": "ktp",
    "label": "Upload KTP",
    "module": "Payroll Indonesia",
    "name": "Employee-upload_ktp"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "tax_identifier_col_break",
    "fieldtype": "Column Break",
    "insert_after": "upload_ktp",
    "module": "Payroll Indonesia",
    "name": "Employee-tax_identifier_col_break"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "npwp_suami",
    "fieldtype": "Data",
    "insert_after": "tax_identifier_col_break",
    "label": "NPWP Suami",
    "module": "Payroll Indonesia",
    "name": "Employee-npwp_suami",
    "depends_on": "eval:doc.gender=='Female'"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "npwp_gabung_suami",
    "fieldtype": "Check",
    "insert_after": "npwp_suami",
    "label": "NPWP Gabung Suami",
    "module": "Payroll Indonesia",
    "name": "Employee-npwp_gabung_suami",
    "depends_on": "eval:doc.gender=='Female'",
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "bpjs_section",
    "fieldtype": "Section Break",
    "insert_after": "health_insurance_provider",
    "label": "BPJS Information",
    "module": "Payroll Indonesia",
    "name": "Employee-bpjs_section"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "ikut_bpjs_kesehatan",
    "fieldtype": "Check",
    "insert_after": "bpjs_section",
    "label": "Ikut BPJS Kesehatan",
    "module": "Payroll Indonesia",
    "name": "Employee-ikut_bpjs_kesehatan",
    "default": "1"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "ikut_bpjs_ketenagakerjaan",
    "fieldtype": "Check",
    "insert_after": "ikut_bpjs_kesehatan",
    "label": "Ikut BPJS Ketenagakerjaan",
    "module": "Payroll Indonesia",
    "name": "Employee-ikut_bpjs_ketenagakerjaan",
    "default": "1"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "tipe_karyawan",
    "fieldtype": "Select",
    "insert_after": "employment_type",
    "label": "Tipe Karyawan",
    "module": "Payroll Indonesia",
    "name": "Employee-tipe_karyawan",
    "options": "Tetap\nTidak Tetap\nFreelance"
  },
  {
    "doctype": "Custom Field",
    "dt": "Employee",
    "fieldname": "penghasilan_final",
    "fieldtype": "Check",
    "insert_after": "tipe_karyawan",
    "label": "Penghasilan Final",
    "module": "Payroll Indonesia",
    "name": "Employee-penghasilan_final",
    "default": "0"
  },
  
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "payroll_indo_section",
    "fieldtype": "Section Break",
    "insert_after": "net_pay",
    "label": "Payroll Indonesia",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-payroll_indo_section",
    "collapsible": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "is_final_gabung_suami",
    "fieldtype": "Check",
    "insert_after": "payroll_indo_section",
    "label": "Is Final (NPWP Gabung Suami)",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-is_final_gabung_suami",
    "default": "0",
    "read_only": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "is_using_ter",
    "fieldtype": "Check",
    "insert_after": "is_final_gabung_suami",
    "label": "Using TER Method",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-is_using_ter",
    "read_only": 1,
    "default": "0"
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "ter_category",
    "fieldtype": "Data",
    "insert_after": "is_using_ter",
    "label": "TER Category",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-ter_category",
    "read_only": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "tax_col_break",
    "fieldtype": "Column Break",
    "insert_after": "ter_category",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-tax_col_break"
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "koreksi_pph21",
    "fieldtype": "Currency",
    "insert_after": "tax_col_break",
    "label": "Koreksi PPh 21 Desember",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-koreksi_pph21",
    "read_only": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "ter_rate",
    "fieldtype": "Float",
    "insert_after": "koreksi_pph21",
    "label": "TER Rate",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-ter_rate",
    "read_only": 1,
    "precision": 2
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "ter_calculation_note",
    "fieldtype": "Small Text",
    "insert_after": "ter_rate",
    "label": "TER Calculation Note",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-ter_calculation_note",
    "read_only": 1,
    "depends_on": "eval:doc.is_using_ter==1"
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "tax_details_section",
    "fieldtype": "Section Break",
    "insert_after": "ter_calculation_note",
    "label": "Tax Details",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-tax_details_section"
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "biaya_jabatan",
    "fieldtype": "Currency",
    "insert_after": "tax_details_section",
    "label": "Biaya Jabatan",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-biaya_jabatan",
    "read_only": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "netto",
    "fieldtype": "Currency",
    "insert_after": "biaya_jabatan",
    "label": "Penghasilan Netto",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-netto",
    "read_only": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "details_col_break",
    "fieldtype": "Column Break",
    "insert_after": "netto",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-details_col_break"
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "npwp",
    "fieldtype": "Data",
    "insert_after": "details_col_break",
    "label": "NPWP",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-npwp",
    "read_only": 1,
    "fetch_from": "employee.npwp"
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "ktp",
    "fieldtype": "Data",
    "insert_after": "npwp",
    "label": "KTP",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-ktp",
    "read_only": 1,
    "fetch_from": "employee.ktp"
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "total_bpjs",
    "fieldtype": "Currency",
    "insert_after": "ktp",
    "label": "Total BPJS",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-total_bpjs",
    "read_only": 1
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "payroll_note_section",
    "fieldtype": "Section Break",
    "insert_after": "total_bpjs",
    "label": "Payroll Notes",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-payroll_note_section"
  },
  {
    "doctype": "Custom Field",
    "dt": "Salary Slip",
    "fieldname": "payroll_note",
    "fieldtype": "Small Text",
    "insert_after": "payroll_note_section",
    "label": "Payroll Note",
    "module": "Payroll Indonesia",
    "name": "Salary Slip-payroll_note",
    "read_only": 1
  }
]